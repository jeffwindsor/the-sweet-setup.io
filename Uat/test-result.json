{
  "data": {
    "description": "The Sweet Setup's Queryable Graph",
    "header": "#!/bin/sh",
    "script": [
      "echo -e '==> Test Section'",
      "name=value",
      "sudo pacman -S --noconfirm arch",
      "yay -S --noconfirm aur",
      "brew install brew",
      "brew cask install cask",
      "npm install npm",
      "code --install-extension vsc",
      "stack install stack",
      "echo -e 'function myfunction(){\nsomestuff\n}' >> /user/home/.bashrc",
      "echo -e 'function myfunction\nsomestuff\nend' > /user/home/.config/fish/functions/myfunction.fish",
      "git config --global alias ga 'git action'",
      "git config --global user.name 'yahoo serious'",
      "git clone http://url.git",
      "git clone http://url-target.git /user/home/two-x",
      "git clone http://url-target-args.git /user/home/two-x --depth=1",
      "git clone http://url-args.git --depth=1",
      "curl http://url.git",
      "curl -L http://url-args.git",
      "curl -L http://url-args-target.git | sh",
      "curl http://url-target.git | sh",
      "echo -e ':set prompt '\\ESC[38;5;242m\\STX%s\n\\ESC[38;5;161m❯\\ESC[1;34mλ= \\ESC[0m'' > ~/.ghci",
      "git config --global s 'status -sb --ignore-submodules'",
      "git config --global d 'diff'",
      "git config --global aa 'add --all'",
      "git config --global cm 'commit -m'",
      "git config --global cma 'commit -a -m'",
      "git config --global b 'branch'",
      "git config --global co 'checkout'",
      "git config --global cob 'checkout -b'",
      "git config --global pff 'pull --ff-only'",
      "git config --global mff 'merge --ff-only'",
      "git config --global size 'count-objects -vH'",
      "git config --global remove 'rm -r --cached .'",
      "git config --global hist 'log --graph --max-count=100 --pretty=format:\\\"%C(green)%h%C(reset) | %C(yellow)%d%C(reset) %s %C(cyan)%an : %C(dim)%cr%C(reset)\\\" --abbrev-commit'",
      "echo -e 'function gs(){\ngit status -sb --ignore-submodules\n}' >> /user/home/.bashrc",
      "echo -e 'function gd(){\ngit diff\n}' >> /user/home/.bashrc",
      "echo -e 'function gaa(){\ngit add --all\n}' >> /user/home/.bashrc",
      "echo -e 'function gcm(){\ngit commit -m\n}' >> /user/home/.bashrc",
      "echo -e 'function gcma(){\ngit commit -a -m\n}' >> /user/home/.bashrc",
      "echo -e 'function gb(){\ngit branch\n}' >> /user/home/.bashrc",
      "echo -e 'function gco(){\ngit checkout\n}' >> /user/home/.bashrc",
      "echo -e 'function gcob(){\ngit checkout -b\n}' >> /user/home/.bashrc",
      "echo -e 'function gpff(){\ngit pull --ff-only\n}' >> /user/home/.bashrc",
      "echo -e 'function gmff(){\ngit merge --ff-only\n}' >> /user/home/.bashrc",
      "echo -e 'function gsize(){\ngit count-objects -vH\n}' >> /user/home/.bashrc",
      "echo -e 'function gremove(){\ngit rm -r --cached .\n}' >> /user/home/.bashrc",
      "echo -e 'function ghist(){\ngit log --graph --max-count=100 --pretty=format:\\\"%C(green)%h%C(reset) | %C(yellow)%d%C(reset) %s %C(cyan)%an : %C(dim)%cr%C(reset)\\\" --abbrev-commit\n}' >> /user/home/.bashrc",
      "echo -e 'function gs\ngit status -sb --ignore-submodules\nend' > /user/home/.config/fish/functions/gs.fish",
      "echo -e 'function gd\ngit diff\nend' > /user/home/.config/fish/functions/gd.fish",
      "echo -e 'function gaa\ngit add --all\nend' > /user/home/.config/fish/functions/gaa.fish",
      "echo -e 'function gcm\ngit commit -m\nend' > /user/home/.config/fish/functions/gcm.fish",
      "echo -e 'function gcma\ngit commit -a -m\nend' > /user/home/.config/fish/functions/gcma.fish",
      "echo -e 'function gb\ngit branch\nend' > /user/home/.config/fish/functions/gb.fish",
      "echo -e 'function gco\ngit checkout\nend' > /user/home/.config/fish/functions/gco.fish",
      "echo -e 'function gcob\ngit checkout -b\nend' > /user/home/.config/fish/functions/gcob.fish",
      "echo -e 'function gpff\ngit pull --ff-only\nend' > /user/home/.config/fish/functions/gpff.fish",
      "echo -e 'function gmff\ngit merge --ff-only\nend' > /user/home/.config/fish/functions/gmff.fish",
      "echo -e 'function gsize\ngit count-objects -vH\nend' > /user/home/.config/fish/functions/gsize.fish",
      "echo -e 'function gremove\ngit rm -r --cached .\nend' > /user/home/.config/fish/functions/gremove.fish",
      "echo -e 'function ghist\ngit log --graph --max-count=100 --pretty=format:\\\"%C(green)%h%C(reset) | %C(yellow)%d%C(reset) %s %C(cyan)%an : %C(dim)%cr%C(reset)\\\" --abbrev-commit\nend' > /user/home/.config/fish/functions/ghist.fish"
    ]
  }
}